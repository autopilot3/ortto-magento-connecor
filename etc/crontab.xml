<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="ortto">
        <job name="ortto_connector_sync_customers" instance="Ortto\Connector\Cron\SyncCustomers"
             method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="ortto_connector_sync_orders" instance="Ortto\Connector\Cron\SyncOrders"
             method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="ortto_connector_sync_products" instance="Ortto\Connector\Cron\SyncProducts"
             method="execute">
            <schedule>* * * * *</schedule>
        </job>
    </group>
</config>
